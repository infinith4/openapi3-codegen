{
    "name": "Python: Connexion",
    "type": "python",
    "request": "launch",
    "module": "connexion",
    "cwd": "${workspaceFolder}",
    "env": {
        "FLASK_APP": "__main__.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "1"
    },
    "args": [
        "run",
        "./output/python-flask/openapi/openapi.yaml",
        "--port",
        "8000"
    ]
}